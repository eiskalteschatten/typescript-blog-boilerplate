{
  "name": "typescript-blog-boilerplate",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "test": "npm run lint",
    "lint": "eslint \"./**/*.{tsx,ts,js,json}\"",
    "prebuild": "webpack",
    "build": "tsc",
    "postbuild": "sass assets/styles:public/dist/css --style compressed",
    "dev:backend": "cross-env NODE_ENV=development ts-node-dev -r tsconfig-paths/register --respawn --transpile-only --rs --inspect=0.0.0.0:9229 -- src/index.ts",
    "dev:frontend": "webpack --watch",
    "dev:scss": "sass --watch assets/styles:public/dist/css",
    "dev": "concurrently  \"npm run dev:backend\" \"npm run dev:frontend\" \"npm run dev:scss\"",
    "prestart": "node ./dist/bootstrap.js",
    "start": "node ./dist/index.js",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eiskalteschatten/typescript-blog-boilerplate.git"
  },
  "author": "Alex Seifert",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/eiskalteschatten/typescript-blog-boilerplate/issues"
  },
  "homepage": "https://github.com/eiskalteschatten/typescript-blog-boilerplate#readme",
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/config": "^3.3.0",
    "@types/ejs": "^3.1.1",
    "@types/node": "^18.11.18",
    "@types/uuid": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^5.48.0",
    "@typescript-eslint/parser": "^5.48.0",
    "concurrently": "^7.6.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.31.0",
    "eslint-import-resolver-typescript": "^3.5.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-promise": "^6.1.1",
    "husky": "^8.0.3",
    "sass": "^1.57.1",
    "ts-loader": "^9.4.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.9.4",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1"
  },
  "dependencies": {
    "@fastify/autoload": "^5.7.0",
    "@fastify/helmet": "^10.1.0",
    "@fastify/multipart": "^7.3.0",
    "@fastify/passport": "^2.2.0",
    "@fastify/secure-session": "^5.3.0",
    "@fastify/static": "^6.6.0",
    "@fastify/view": "^7.3.0",
    "bcryptjs": "^2.4.3",
    "config": "^3.3.8",
    "dotenv": "^16.0.3",
    "ejs": "^3.1.8",
    "fastify": "^4.11.0",
    "passport": "^0.6.0",
    "passport-local": "^1.0.0",
    "pg": "^8.8.0",
    "pg-hstore": "^2.3.4",
    "pino": "^8.8.0",
    "pino-pretty": "^9.1.1",
    "redis": "^4.5.1",
    "sequelize": "^6.28.0",
    "sequelize-migration-wrapper": "^1.0.1",
    "uuid": "^9.0.0"
  }
}
